<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <session-config>
        <session-timeout>
            100000
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>rc</servlet-name>
        <servlet-class>com.servlets.Servlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rc</servlet-name>
        <url-pattern>/rc.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>rac</servlet-name>
        <servlet-class>com.servlets.Servlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>rac</servlet-name>
        <url-pattern>/rac.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>transferSv</servlet-name>
        <servlet-class>com.servlets.trasferSv</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>transferSv</servlet-name>
        <url-pattern>/transfer.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.servlets.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    

</web-app>
